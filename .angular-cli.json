{
  "$schema": "./node_modules/@nrwl/schematics/src/schema.json",
  "project": {
    "name": "morningharwood",
    "npmScope": "suite",
    "latestMigration": "20180130-angular-devkit-schematics"
  },
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "./tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/morningharwood/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/morningharwood/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/morningharwood/src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "apps/morningharwood/e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "schematics": {
      "collection": "@nrwl/schematics",
      "postGenerate": "npm run format",
      "newProject": [
        "app",
        "lib"
      ]
    },
    "styleExt": "scss",
    "component": {
      "changeDetection": "OnPush"
    }
  },
  "warnings": {
    "typescriptMismatch": false
  },
  "apps": [
    {
      "name": "morningharwood",
      "root": "apps/morningharwood/src",
      "outDir": "dist/apps/morningharwood",
      "assets": [
        "assets/images",
        "favicon.ico",
        "apple-touch-icon.png",
        "favicon-32x32.png",
        "favicon-16x16.png",
        "manifest.json",
        "safari-pinned-tab.svg",
        "humans.txt",
        "license.txt",
        "404.html",
        "android-chrome-256x256.png",
        "android-chrome-192x192.png",
        "mstile-150x150.png",
        "browserconfig.xml"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "morningharwood-server",
      "platform": "server",
      "root": "apps/morningharwood/src",
      "outDir": "functions/dist-server",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.server.ts",
      "test": "../../../test.js",
      "tsconfig": "tsconfig.server.json",
      "testTsconfig": "../../../tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    },
    {
      "name": "lsroutes",
      "root": "libs/lsroutes/src",
      "test": "../../../test.js",
      "appRoot": ""
    },
    {
      "name": "prerender",
      "root": "libs/prerender/src",
      "test": "../../../test.js",
      "appRoot": ""
    }
  ]
}
